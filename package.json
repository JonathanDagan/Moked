{
  "name": "moked",
  "version": "0.2.1",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:frontend": "yarn --cwd frontend run dev",
    "dev:backend": "yarn --cwd backend run start:dev",
    "lint": "yarn --cwd frontend run lint",
    "open-ct": "yarn --cwd frontend run cypress open-ct",
    "postinstall": "(cd frontend && yarn install); && (cd ../backend && yarn install);"
  },
  "dependencies": {},
  "devDependencies": {
    "husky": "^7.0.0",
    "yarn-run-all": "^3.1.1"
  }
}